{
  "name": "NCKU-CSIE-Website",
  "version": "1.1.2",
  "description": "NCKU CSIE Website",
  "private": true,
  "scripts": {
    "develop": "npm-run-all --parallel nodemon watch browser-sync",
    "nodemon": "nodemon --config dev/server/nodemon.json server.js",
    "browser-sync": "browser-sync --config dev/server/bs-config.js start",
    "pre-build": "npm-run-all pre-build:*",
    "pre-build:database": "shx cp settings/database/config.js.default settings/database/config.js",
    "pre-build:server": "shx cp settings/server/config.js.default settings/server/config.js",
    "build": "npm-run-all build:css build:html build:js",
    "build:css": "webpack --config dev/css/webpack.config.js",
    "build:html": "webpack --config dev/html/webpack.config.js",
    "build:js": "webpack --config dev/js/webpack.config.js",
    "build:database": "node dev/database/build.js",
    "clean": "npm-run-all --parallel clean:*",
    "clean:css": "shx rm -rf static/dist/css",
    "clean:html": "shx rm -rf static/dist/html",
    "clean:js": "shx rm -rf static/dist/js",
    "lint": "npm-run-all --parallel lint:*",
    "lint:css": "stylelint --config dev/css/.stylelintrc.js --cache --cache-location node_modules/.cache/.stylelintcache --syntax scss --fix \"static/src/sass/**/*.scss\"",
    "lint:html": "pug-lint --config dev/html/.pug-lintrc.js static/src/pug",
    "lint:js": "eslint --config dev/js/.eslintrc.js --cache --cache-location node_modules/.cache/.eslintcache-js --fix \"static/src/js/**/*.js\"",
    "lint:server": "eslint --config dev/server/.eslintrc.js --cache --cache-location node_modules/.cache/.eslintcache-server --ignore-path dev/server/.eslintignore --fix \"apis/**/*.js\" \"dev/**/*.js\" \"models/**/*.js\" \"routes/**/*.js\" \"settings/**/*.js\" \"static/src/language/**/*.js\" server.js",
    "watch": "npm-run-all --parallel watch:*",
    "watch:css": "webpack --watch --config dev/css/webpack.config.js",
    "watch:html": "webpack --watch --config dev/html/webpack.config.js",
    "watch:js": "webpack --watch --config dev/js/webpack.config.js",
    "d": "npm run develop",
    "p": "npm run pre-build",
    "pd": "npm run pre-build:database",
    "ps": "npm run pre-build:server",
    "b": "npm run build",
    "bc": "npm run build:css",
    "bh": "npm run build:html",
    "bj": "npm run build:js",
    "bd": "npm run build:database",
    "c": "npm run clean",
    "cc": "npm run clean:css",
    "ch": "npm run clean:html",
    "cj": "npm run clean:js",
    "l": "npm run lint",
    "lc": "npm run lint:css",
    "lh": "npm run lint:html",
    "lj": "npm run lint:js",
    "ls": "npm run lint:server",
    "w": "npm run watch",
    "wc": "npm run watch:css",
    "wh": "npm run watch:html",
    "wj": "npm run watch:js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toolbuddy/NCKU-CSIE-Website.git"
  },
  "author": "ProFatXuanAll",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/toolbuddy/NCKU-CSIE-Website/issues"
  },
  "homepage": "https://github.com/toolbuddy/NCKU-CSIE-Website#readme",
  "dependencies": {
    "deep-freeze": "0.0.1",
    "express": "^4.16.3",
    "load-google-maps-api": "^1.3.2",
    "mysql": "^2.16.0",
    "mysql2": "^1.5.3",
    "node": "^10.6.0",
    "node-sass": "^4.9.2",
    "npm": "^6.2.0",
    "pug": "^2.0.3",
    "sequelize": "^4.38.0"
  },
  "devDependencies": {
    "browser-sync": "^2.24.5",
    "css-loader": "^1.0.0",
    "cssnano": "^4.0.2",
    "eslint": "^5.1.0",
    "eslint-loader": "^2.0.0",
    "extract-loader": "^2.0.1",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "mini-css-extract-plugin": "^0.4.1",
    "nodemon": "^1.18.2",
    "npm-run-all": "^4.1.3",
    "postcss-loader": "^2.1.6",
    "pug-html-loader": "^1.1.5",
    "pug-lint": "^2.5.0",
    "pug-loader": "^2.4.0",
    "sass-loader": "^7.0.3",
    "sequelize-auto": "^0.4.29",
    "shx": "^0.3.2",
    "style-loader": "^0.21.0",
    "stylelint": "^9.3.0",
    "stylelint-scss": "^3.1.3",
    "stylelint-webpack-plugin": "^0.10.5",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.0.8"
  }
}
