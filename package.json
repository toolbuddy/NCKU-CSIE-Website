{
  "name": "NCKU-CSIE-Website",
  "version": "1.1.1",
  "description": "NCKU CSIE Website",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch:sass": "node-sass static/src/sass/about/teachers.scss  static/dist/css/about/teachers.css -w",
    "compile:sass": "node-sass static/src/sass/announcement/all.scss static/dist/css/announcement/all.css",
    "eslint-frontend": "eslint --ignore-path ./settings/lint/eslint/.frontend_lint_ignore -c ./settings/lint/eslint/frontend_lint.js ./static/src/js/* --quiet; exit 0;",
    "eslint-frontend-fix": "eslint --ignore-path ./settings/lint/eslint/.frontend_lint_ignore -c ./settings/lint/eslint/frontend_lint.js ./static/src/js/* --quiet --fix; exit 0;",
    "eslint-backend": "eslint --ignore-path settings/lint/eslint/.backend_lint_ignore -c settings/lint/eslint/backend_lint.js * --quiet; exit 0;",
    "eslint-backend-fix": "eslint --ignore-path settings/lint/eslint/.backend_lint_ignore -c settings/lint/eslint/backend_lint.js * --quiet --fix; exit 0;",
    "puglint": "pug-lint -c settings/lint/puglint/puglint.js views/*; exit 0",
    "pre-build": "./bin/pre-build.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toolbuddy/NCKU-CSIE-Website.git"
  },
  "author": "ProFatXuanAll",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/toolbuddy/NCKU-CSIE-Website/issues"
  },
  "homepage": "https://github.com/toolbuddy/NCKU-CSIE-Website#readme",
  "dependencies": {
    "express": "^4.16.3",
    "inspect": "0.0.2",
    "mysql": "^2.15.0",
    "mysql2": "^1.5.3",
    "node": "^9.8.0",
    "node-sass": "^4.8.3",
    "npm": "^5.8.0",
    "pug": "^2.0.3",
    "sequelize": "^4.37.6",
    "sequelize-auto": "^0.4.29"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "pug-lint": "^2.5.0",
    "sass-lint": "^1.12.1"
  }
}
