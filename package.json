{
  "name": "webtemplate",
  "version": "1.0.0",
  "description": "ncku csie web",
  "main": "server.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "nodemon server.js",
    "eslint-frontend": "./node_modules/.bin/eslint --ignore-path ./settings/lint/eslint/.frontend_lint_ignore -c ./settings/lint/eslint/frontend_lint.js ./static/src/js/* --quiet; exit 0;",
    "eslint-frontend-fix": "./node_modules/.bin/eslint --ignore-path ./settings/lint/eslint/.frontend_lint_ignore -c ./settings/lint/eslint/frontend_lint.js ./static/src/js/* --quiet --fix; exit 0;",
    "eslint-backend": "./node_modules/.bin/eslint --ignore-path settings/lint/eslint/.backend_lint_ignore -c settings/lint/eslint/backend_lint.js * --quiet; exit 0;",
    "eslint-backend-fix": "./node_modules/.bin/eslint --ignore-path settings/lint/eslint/.backend_lint_ignore -c settings/lint/eslint/backend_lint.js * --quiet --fix; exit 0;",
    "puglint": "pug-lint -c settings/lint/puglint/puglint.js views/*; exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ProFatXuanAll/webtemplate.git"
  },
  "author": "ProFatXuanAll",
