  "bugs": {
    "url": "https://github.com/ProFatXuanAll/webtemplate/issues"
  },
  "homepage": "https://github.com/ProFatXuanAll/webtemplate#readme",
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "node": "^9.8.0",
    "npm": "^5.7.1",
    "pug": "^2.0.0-rc.4"
  },
  "devDependencies": {
    "eslint": "^4.18.2",
    "node-sass": "^4.8.3",
    "nodemon": "^1.17.2",
    "pug-lint": "^2.5.0",
    "sass-lint": "^1.12.1"
  }
}
