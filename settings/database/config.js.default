module.exports = {
    username: '',
    password: '',
    host: 'localhost',
    port: 3306,
    protocol: 'mysql',
    pool: {
        max: 2000,
        min: 0,
        acquire: 0,
        idle: 0,
    },
    url ( databaseName ) {
        return `${ this.protocol }://${ this.username }:${ this.password }@${ this.host }:${ this.port }/${ databaseName }`;
    }
};
